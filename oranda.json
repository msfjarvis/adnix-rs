{
  "project": {
    "name": "adnix-rs"
  },
  "build": {
    "path_prefix": "adnix-rs"
  },
  "components": {
    "changelog": false,
    "artifacts": {
      "auto": true,
      "package_managers": {
        "preferred": {
          "nix flake": "nix profile install github:msfjarvis/adnix-rs",
          "cargo": "cargo install adnix --locked --profile=dist"
        },
        "additional": {
          "binstall": "cargo binstall adnix"
        }
      }
    }
  }
}
